    "style-loader": "^0.23.1",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "svg-sprite-loader": "^4.1.2",
    "ts-jest": "^22.4.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2",
    "webpack-shell-plugin": "^0.5.0"
  },
