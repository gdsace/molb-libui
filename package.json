{
  "name": "molb-libui",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/@types/components/index.d.ts",
  "scripts": {
    "start": "start-storybook -p 6006 -c .storybook",
    "build": "npm run build-lib && build-storybook",
    "build-lib": "tsc && npm run copy-css-to-lib",
    "build-storybook": "build-storybook",
    "clean": "rm -rf dist",
    "copy-css-to-lib": "cpx \"./src/**/*.css\" ./build/lib",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:eslint": "eslint . --ext .js --ext .jsx",
    "lint:fix": "yarn lint:tslint:fix && yarn lint:eslint:fix && yarn lint:stylelint:fix",
    "lint:stylelint:fix": "yarn lint:stylelint --fix",
    "lint:stylelint": "stylelint '**/*.css' '**/*.scss'",
    "lint:tslint:fix": "yarn lint:tslint --fix",
    "lint:tslint": "tslint -c tslint.json --project tsconfig.json",
    "lint": "yarn lint:tslint && yarn lint:eslint && yarn lint:stylelint && yarn tsc:check",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test": "jest",
    "tsc:check": "tsc --noEmit --skipLibCheck",
    "ck": "yarn test && yarn lint",
    "export": "npm run build-lib && build-storybook -c .storybook -o .out",
    "build:type": "tsc --module es6 --declaration --emitDeclarationOnly --outDir dist/@types",
    "build:production": "rm -rf ./dist && cross-env NODE_ENV=production webpack --config webpack.config.js --mode production && yarn build:type",
    "build:check": "yarn build:production && [ -z \"$(git status --porcelain)\" ] || { echo \"Please run 'yarn build:production' and commit the changes\" >&2 && exit 1; }"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/gdsace/molb-libui.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@sambego/storybook-state": "^1.3.1",
    "@storybook/addon-actions": "^3.4.11",
    "@storybook/addon-console": "^1.0.4",
    "@storybook/addon-info": "^3.4.0",
    "@storybook/addon-knobs": "^3.4.0",
    "@storybook/addon-viewport": "^3.4.11",
    "@storybook/react": "^3.4.0",
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^22.2.2",
    "@types/lodash": "^4.14.117",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.3.5",
    "@types/react-dom": "^16.0.9",
    "@types/react-select": "^2.0.10",
    "@types/storybook__addon-info": "^3.2.3",
    "@types/storybook__addon-knobs": "^3.3.1",
    "@types/storybook__react": "^3.0.8",
    "autoprefixer": "^9.4.2",
    "awesome-typescript-loader": "^4.0.1",
    "babel-core": "^6.26.3",
